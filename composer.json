{
  "name": "cri/geocode-osm",
  "type": "typo3-cms-extension",
  "homepage": "https://github.com/christophrunkel/geocode_osm",
  "description": "This extension provides a task/command to generate tt_address geocoding via OpenStreetMap (Nominatim) instead of Google Maps.",
  "authors": [
    {
      "name": "Christoph Runkel",
      "role": "Product Manager"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.1",
    "typo3/cms-core": "^12.4 || ^13.4"
    "friendsoftypo3/tt-address": ">=8.0",
  },
  "autoload": {
    "psr-4": {
      "Cri\\GeocodeOsm\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/geocode-osm": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
    ]
  },
  "extra": {
    "typo3/cms": {
      "web-dir": ".Build/public",
      "extension-key": "geocode_osm"
    }
  }
}
